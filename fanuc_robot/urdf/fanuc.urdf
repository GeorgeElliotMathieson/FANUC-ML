<?xml version="1.0"?>
<robot name="fanuc">

<!-- Base Link -->
<link name="base_link">
    <collision>
        <geometry>
            <mesh filename="../meshes/Part 1.STL" scale="0.001 0.001 0.001" />
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </collision>
    <visual>
        <geometry>
            <mesh filename="../meshes/Part 1.STL" scale="0.001 0.001 0.001" />
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <material name="black">
            <color rgba="0.1 0.1 0.1 1.0"/>
        </material>
    </visual>
    <inertial>
        <origin xyz="0.105 0.09 0.08042" rpy="0 0 0"/>
        <mass value="4.85331"/>
        <inertia ixx="0.000533781" iyy=".055445798" izz=".02302818052" ixy="0.00012915752" ixz="-0.002807595" iyz="-0.000094967"/>
    </inertial>
</link>

<!-- World link -->
<link name="world"/>

<!-- Fixed joint connecting world to base -->
<joint name="world_base_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
</joint>

<!-- Part 2 (First rotating part) -->
<joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <dynamics damping="0.7"/>
    <axis xyz="0 0 1"/>
    <origin xyz="0.105 0.09 0.16859"/>
    <limit lower="-3.14159" upper="3.14159" effort="330.0" velocity="3.14"/>
</joint>

<link name="link1">
    <collision>
        <geometry>
            <mesh filename="../meshes/Part 2.STL" scale="0.001 0.001 0.001"/>
        </geometry>
        <origin xyz="-0.125 -0.110 0" rpy="0 0 0"/>
